      randFactor: 0
      startVal: 4.5
      startValRandFactor: 0
    }
    dPitch: {
      factor: 0.5
    }
    # Spawn rate roughly in half.
    count: 7
    stretch: {
      randFactor: 0
      startVal: 0.6
    }
  }
  caveBlocks: {
    states: lava
    height: [ 0, 10 ]
  }
  # Whether the preset is enabled globally.
  enabled: true
}