  replaceableBlocks: $NATURAL_STONE
  replaceDecorators: true
  caverns: {
    enabled: true # Removed in 1.0. You can delete this field.
    height: [ 16, 58 ]
    generators: {
      frequency: 0.015
      gain: 0.4
      type: Cellular
      returnType: Distance3Div
      jitter: 0.75
      perturb: true
      perturbAmp: 10
      perturbFreq: 0.025
      invert: true
      threshold: -0.4
      stretch: 0.58
